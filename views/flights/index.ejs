<%- include('../partials/header') %>
	<h2>Flights</h2>
	<table>
		<thead>
			<tr>
				<th>Airline</th>
				<th>Airport</th>
				<th>Flight #</th>
				<th>Departs</th>
			</tr>
		</thead>
		<tbody>
			<% flights.forEach(function(f) { %>
				<tr>
					<td>
						<%= f.airline %>
					</td>
					<td>
						<%= f.aiport %>
					</td>
					<td>
						<a href="/flights/<%= f.id %>"><%= f.flightNo %></a>
					</td>
					<td>
						<%= f.departs %>
					</td>
				</tr>
			<% }); %>
		</tbody>
	</table>
<%- include('../partials/footer') %>